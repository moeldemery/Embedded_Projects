#ifndef F_CPU
#define F_CPU 12000000LU
#endif

#include "UTILITIES/STD_TYPES.h"
#include "UTILITIES/BIT_MATH.h"


#include <util/delay.h>



void LED1_On(void);
void LED1_Off(void);

int main(void)
{
	DIO_Init();


	while(1)
	{
		if(BTN_GetValue())
		{
			LED1_Off();
		}
		else
		{
			LED1_On();
		}
	}

	return 0 ;
}



void LED1_On(void)
{
	DIO_Write(0xff,GRP_1_LED);
}

void LED1_Off(void)
{
	DIO_Write(0x00,0);
}











