#ifndef MCAL_INC_UART_DRIVER_H_
#define MCAL_INC_UART_DRIVER_H_
#include "STD_TYPES.h"

typedef struct
{
	u8 				Enable;
	U8				UART_USART;
	U8				dataFrame;
	u8				endingBits;
	u8		 		controlFrame;
	u8				clockPolarity;
	u8				BoadRate;

}UASRT_config_t;

#endif /* MCAL_INC_UART_DRIVER_H_ */
