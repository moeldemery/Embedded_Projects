#include "DIO_interface.h"

#define F_CPU 8000000LU
#include <util/delay.h>

int main(void)
{
	//DDRA = 0b00000001;
	DIO_EnumSetPinDirection(DIO_DDRD,DIO_PIN_0,DIO_PIN_OUTPUT);

	while(1)
	{
		DIO_EnumSetPinValue(DIO_DDRD,DIO_PIN_0,DIO_PIN_HIGH);
		//for(unsigned int i=0 ;i<65000;i++);
		_delay_ms(1000);
		DIO_EnumSetPinValue(DIO_DDRD,DIO_PIN_0,DIO_PIN_LOW);
		_delay_ms(1000);
	}



	return 0 ;
}
