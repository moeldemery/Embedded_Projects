#include "FreeRTOS/FreeRTOS.h"
#include "FreeRTOS/task.h"

void task1_code(void*pvParamter);
 void task2_code(void*pvParamter);

int main(void)
{

	while(1)
	{

	}
	return 0;
}


void task1_code(void*pvParamter)
{
	PORTD ^= 0xff;
	vTaskDelay(1000);

}
 void task2_code(void*pvParamter)
 {
	 PORTD ^= 0xff;
	 vTaskDelay(2000);
 }
