#include "../INC/KPD.h"

u8 KPD_Get_State(void)
{

    unsigned char Col_value,State_busy;

    DIO_Write(0XFF,GRP_13_ROW);
    Col_value = DIO_Read(GRP_14_COL);

        if (Col_value== (0B0000<<KPD_COL_PIN))
        {
            State_busy = 0x00;
        }
        else
        {
            State_busy = 0xFF;
        }


    return State_busy;
}
