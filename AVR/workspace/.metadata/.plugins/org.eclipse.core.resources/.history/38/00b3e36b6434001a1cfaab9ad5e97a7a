#include "INT_interface.h"

void INT_Void_ExternalIntInit(u8 Copy_u8_ChannelNo)
{
	switch (Copy_u8_ChannelNo)
	{
		case EXT_INT_0:
		{

			SET_BIT(MCUCR_REG,EXT_INT_SENCE_FAILING);
			SET_BIT(GICR_REG,EXT_INT_0);
			SET_BIT(SREG_REG,7);

			DIO_EnumSetPinDirection(DIO_PORT(4),2,DIO_PIN_INPUT);
			break;
		}

		default:
		{

			break;
		}

	}
}
